{
  "name": "news-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app",
    "install": "npm install && npm install --prefix client",
    "server": "nodemon app",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "deploy": "npm run build --prefix client && npm run deploy --prefix client"
  },
  "dependencies": {
    "concurrently": "^4.1.0",
    "express": "~4.16.0"
  }
}
